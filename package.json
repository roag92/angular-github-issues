{
  "name": "angular-github-issues",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=local",
    "build": "ng build --prod",
    "test": "ng test --watch false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:fix": "prettier --config ./.pretierrc --write \"src/**/*{.ts,.json,.css}\"",
    "format:check": "prettier --config ./.pretierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.json,.css}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "src/**/*{.ts,.json,.css}": [
      "npm run format:fix",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.0",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^7.2.0",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "ace": "ajaxorg/ace#master",
    "core-js": "^2.6.1",
    "firebase": "^5.7.2",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.13.1",
    "marked": "^0.6.0",
    "ngx-markdown-editor": "^1.1.11",
    "rxjs": "^6.3.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.1",
    "@angular/cli": "~6.2.5",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^7.2.0",
    "@types/jasmine": "^3.3.5",
    "@types/jasminewd2": "^2.0.6",
    "@types/marked": "^0.6.0",
    "@types/node": "^10.12.18",
    "codelyzer": "^4.5.0",
    "husky": "^1.3.1",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "protractor": "^5.4.2",
    "ts-node": "~7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^2.9.2"
  }
}
